{
    "name": "vinelab/neoeloquent-support",
    "description": "Fork of the Illuminate Support package to be used with Vinelab/NeoEloquent.",
    "license": "MIT",
    "homepage": "https://github.com/vinelab/NeoEloquent",
    "support": {
        "issues": "https://github.com/vinelab/NeoEloquent/issues",
        "source": "https://github.com/vinelab/NeoEloquent"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylorotwell@gmail.com"
        },
        {
            "name": "Abed Halawi",
            "email": "abed.halawi@vinelab.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "ext-mbstring": "*",
        "vinelab/neoeloquent-contracts": "5.2.*",
        "doctrine/inflector": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Vinelab\\NeoEloquent\\Support\\": ""
        },
        "files": [
            "helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.2-dev"
        }
    },
    "suggest": {
        "jeremeamia/superclosure": "Required to be able to serialize closures (~2.2).",
        "paragonie/random_compat": "Provides a compatible interface like PHP7's random_bytes() in PHP 5 projects (~1.1).",
        "symfony/polyfill-php56": "Required to use the hash_equals function on PHP 5.5 (~1.0).",
        "symfony/var-dumper": "Improves the dd function (2.8.*|3.0.*)."
    },
    "minimum-stability": "dev"
}
